  "license": "AGPL-3.0",
  "engines": {
    "node": ">= 8.12.0"
  },
  "repository": "millette/find-putzi",
  "author": {
    "name": "Robin Millette",
    "email": "robin@millette.info",
    "url": "http://robin.millette.info"
  },
  "lint-staged": {
    "*.{js,json,md}": ["prettier-standard", "git add"]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "gatsby": "^2.0.91",
    "gatsby-plugin-manifest": "^2.0.13",
    "gatsby-plugin-offline": "^2.0.21",
    "gatsby-plugin-react-helmet": "^3.0.5",
    "gatsby-source-filesystem": "^2.0.16",
    "gatsby-transformer-json": "^2.1.7",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier-standard": "^8.0.1",
    "standard": "^12.0.1"
  },
  "scripts": {
    "deploy":
      "rm public/* .cache -fr && gatsby build --prefix-paths && gh-pages -d public",
    "lint": "standard",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "start": "gatsby develop --port 8001",
    "clean": "rm public/* .cache -fr",
    "format": "prettier-standard '**/*.+(js|json|md)'"
  }
}
